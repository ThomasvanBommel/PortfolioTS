{
  "name": "portfoliots",
  "version": "1.0.0",
  "description": "Portfolio written with typescript",
  "main": "index.js",
  "scripts": {
    "prebuild-client": "rm -rf client/build",
    "build-client": "webpack --config ./config/webpack.config.js",
    "watch": "npm run build-client -- -w --env --dev",
    "prebuild-server": "rm -rf server/build",
    "build-server": "tsc -p server",
    "build": "npm run build-client && npm run build-server",
    "start": "node -r dotenv/config server/build/index.js dotenv_config_path=./config/config.env",
    "test": "npm run build && npm start -- --test",
    "posttest": "npm run prebuild-client && npm run prebuild-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThomasvanBommel/PortfolioTS.git"
  },
  "author": "Thomas vanBommel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThomasvanBommel/PortfolioTS/issues"
  },
  "homepage": "https://github.com/ThomasvanBommel/PortfolioTS#readme",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-slick": "^0.28.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  }
}
