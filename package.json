{
  "name": "portfoliots",
  "version": "1.0.0",
  "description": "Portfolio written with typescript",
  "main": "index.js",
  "scripts": {
    "prebuild-client": "rm -rf client/build",
    "build-client": "webpack --config ./config/webpack.config.js",
    "watch": "npm run build-client -- -w --env --dev",
    "prebuild-server": "rm -rf server/build",
    "build-server": "tsc -p server",
    "prestart": "npm run build-server && npm run test && npm run build-client",
    "start": "node server/build/index.js",
    "test": "npm run build-server && node server/build/index.js --test && npm run build-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThomasvanBommel/PortfolioTS.git"
  },
  "author": "Thomas vanBommel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThomasvanBommel/PortfolioTS/issues"
  },
  "homepage": "https://github.com/ThomasvanBommel/PortfolioTS#readme",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "css-loader": "^5.0.2",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.1",
    "mocha": "^8.3.0"
  }
}
